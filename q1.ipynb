    "arr1 = np.array([[4, 5, 6],[6, 7, 8]])\n",
    "\n",
    "m=np.multiply(arr,arr1)\n",
    "print(m)\n",
    "n=np.divide(arr,arr1)\n",
    "print(n)\n",
    "print(arr+arr1)\n",
    "k=np.power(arr,arr1)\n",
    "print(k)"
